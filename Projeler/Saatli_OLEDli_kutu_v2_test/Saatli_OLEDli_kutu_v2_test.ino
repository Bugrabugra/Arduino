void printTemperature()
{
  byte temperature = 0;
  byte humidity = 0;
  int err = SimpleDHTErrSuccess;

  if ((err = dht11.read(&temperature, &humidity, NULL)) != SimpleDHTErrSuccess)
  {
    display2.print(err);
    return;
  }

  display2.clearDisplay();
  display2.setTextSize(3);
  display2.setTextColor(WHITE);
  display2.cp437(true);

  display2.drawBitmap(0, 25, thermo, 32, 32, 1);

  display2.setCursor(45, 30);
  display2.print((int)temperature); display2.print(" C ");

  display2.setTextSize(2);
  display2.setCursor(80, 26);
  display2.print("o");

  display2.display();
  delay(500);
}


void printClock(byte second, byte minute, byte hour, byte dayOfWeek, byte dayOfMonth, byte month, byte year)
{
  display2.clearDisplay();
  display2.setTextSize(2.5);
  display2.setTextColor(WHITE);
  display2.cp437(true);
  display2.setCursor(0, 40);

  if (hour < 10)
  {
    display2.print("0");
  }
  display2.print(hour);
  display2.print(':');

  if (minute < 10)
  {
    display2.print("0");
  }
  display2.print(minute);
  display2.print(':');

  if (second < 10)
  {
    display2.print("0");
  }
  display2.print(second);
  display2.setCursor(32, 2);
  display2.setTextSize(2);

  if (dayOfMonth < 10)
  {
    display2.print("0");
  }
  display2.print(dayOfMonth);
  display2.print('/');

  if (month < 10)
  {
    display2.print("0");
  }
  display2.print(month);
  display2.print('/');
  display2.print(year);
  display2.drawBitmap(0, 0, calendar, 32, 32, 1);
  display2.drawBitmap(96, 30, clock_img, 32, 32, 1);

  display2.display();
}


